{
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  // HERE
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "prettier"
  ],

  "rules": {
    "no-unused-vars": "error", //
    "no-console": "warn", //no allow for testing console push in git
    "no-undef": "error", //undefined value hole error dibe
    "no-unused-expressions": "error", //unused kono  expression hole error dibe
    "no-unreachable": "error", //unreachable code hole error dibe
    // "consistent-type-definitions": ["error", "type"]
    "@typescript-eslint/consistent-type-definitions": "error"
  },

  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "globals": {
    "process": "readonly"
  }
}
